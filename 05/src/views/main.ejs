<!DOCTYPE html>
<head>
  <meta charset="utf-8">
  <title>main</title>
</head>
<body>
  <h1>Main</h1>
  <p><strong><%= loginUser %></strong> でログイン中</p>
  <h3>User List</h3>
  <ul id="user_list">
    <% users.forEach(function(user) { %>
      <li><a href="/user?username=<%= user.username %>"><%= user.username %></a></li>
    <% }) %>
  </ul>
</body>